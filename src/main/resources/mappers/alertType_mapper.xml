<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.bewellnesspring.alert.model.repository.AlertTypeMapper">

    <insert id="insertAlertType">

    </insert>

    <select id="findAll">
        select * from alert_Type;
    </select>

    <select id="findById" parameterType="Long" resultType="AlertType" >
        select * from alert_Type where id = #{typeId}
    </select>

    <select id="findByAltype" resultType="AlertType">
        select * from alert_Type where altype = #{altype}
    </select>


</mapper>